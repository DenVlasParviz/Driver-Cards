{"ast":null,"code":"import { mapWeatherCodeToIcon } from \"@/store/Weather/weatherIcons\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      resultData: []\n    };\n  },\n  computed: {\n    ...mapGetters(\"vehicles\", [\"getVehicleInfo\"])\n  },\n  methods: {\n    mapIcon(code) {\n      return mapWeatherCodeToIcon(code);\n    },\n    ...mapActions(\"vehicles\", [\"removeVehicle\"]),\n    onSubmit(id) {\n      this.removeVehicle(id);\n    }\n  }\n};","map":{"version":3,"names":["mapWeatherCodeToIcon","mapActions","mapGetters","data","resultData","computed","methods","mapIcon","code","onSubmit","id","removeVehicle"],"sources":["src/components/AboutView.vue"],"sourcesContent":["<template>\n  <b-container>\n    <b-row class=\"justify-content-center\">\n      <h1 v-if=\"getVehicleInfo.length === 0\">Картки ще не створені</h1>\n      <b-card\n        v-for=\"(result) in getVehicleInfo\"\n        :key=\"result.id\"\n        border-variant=\"primary\"\n        header-bg-variant=\"primary\"\n        bg-variant=\"result.isNight? 'dark\"\n        text-variant=\"white\"\n        header-text-variant=\"white\"\n        align=\"center\"\n        class=\"text-center mt-4 m-2\"\n        style=\"width: 20rem\"\n      >\n        <h5>Ім'я водія: {{ result.driver_name }}</h5>\n        <h5>Номер авто: {{ result.driver_number }}</h5>\n        <h5>Вага авто: {{ result.car_weight }}</h5>\n        <h5>температура : {{ result.weather_temp }} °C</h5>\n        <p>\n          <b-img\n            class=\"weather-image\"\n            :src=\"mapIcon(result.weather_code)\"\n          ></b-img>\n        </p>\n        <b-button variant=\"danger\" @click.prevent=\"onSubmit(result.id)\">\n          Видалити картку</b-button\n        >\n      </b-card>\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport { mapWeatherCodeToIcon } from \"@/store/Weather/weatherIcons\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      resultData: [],\n    };\n  },\n  computed: {\n    ...mapGetters(\"vehicles\", [\"getVehicleInfo\"]),\n  },\n\n  methods: {\n    mapIcon(code) {\n      return mapWeatherCodeToIcon(code);\n    },\n    ...mapActions(\"vehicles\", [\"removeVehicle\"]),\n    onSubmit(id) {\n      this.removeVehicle(id);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.weather-image {\n  width: 36px;\n  height: 36px;\n}\n</style>\n"],"mappings":"AAmCA,SAAAA,oBAAA;AACA,SAAAC,UAAA,EAAAC,UAAA;AACA;EACAC,KAAA;IACA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAH,UAAA;EACA;EAEAI,OAAA;IACAC,QAAAC,IAAA;MACA,OAAAR,oBAAA,CAAAQ,IAAA;IACA;IACA,GAAAP,UAAA;IACAQ,SAAAC,EAAA;MACA,KAAAC,aAAA,CAAAD,EAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}